{
  "name": "whiteboard",
  "scripts": {
    "generate-schema": "typescript-json-schema --required --strictNullChecks src/schema.d.ts '*' --out src/schema.json && npm run format",
    "format": "prettier 'src/**/*.{ts,json,html}' 'test/**/*.{ts,json}' --write",
    "build": "esbuild --tsconfig=src/tsconfig.json src/index.ts --bundle --sourcemap --loader:.html=binary --format=esm --outfile=dist/index.mjs",
    "test": "npm run build && tsc -p test/tsconfig.json && mocha -r source-map-support/register"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cfworker/json-schema": "^1.12.1",
    "@cloudflare/workers-types": "^3.3.1",
    "@cloudflare/wrangler": "^1.19.7",
    "@types/cookie": "^0.4.1",
    "@types/deep-equal": "^1.0.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.14",
    "@types/node-fetch": "^2.5.12",
    "@types/ws": "^8.2.2",
    "cookie": "^0.4.2",
    "deep-equal": "^2.0.5",
    "esbuild": "^0.14.13",
    "itty-router": "^2.4.10",
    "miniflare": "^2.2.0",
    "mocha": "^9.2.0",
    "node-fetch": "^2.6.7",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "tree-kill": "^1.2.2",
    "typescript": "^4.6.0-dev.20220201",
    "typescript-json-schema": "^0.53.0",
    "ws": "^8.4.2"
  }
}
